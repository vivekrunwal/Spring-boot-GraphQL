schema {
    query: Query
}
type Actor{

      actorId:ID!,
      firstName:String,
      lastName:String,

      address:String,
      dob:String,
      filmId:Int,
      film:Film
}

type Film{


  filmId:ID!,

  name:String,

  launchYear:String
}

type Query{
    getAllActors:[Actor],
    getActorById(id:Int):Actor
}

